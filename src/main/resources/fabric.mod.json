{
	"schemaVersion": 1,
	"id": "lover",
	"version": "${version}",
	"name": "Lover",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/lover/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.chsteam.lover.Lover",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.chsteam.lover.LoverDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"lover.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.20"
	},
	"suggests": {
		"another-mod": "*"
	},
	"accessWidener": "lover.accesswidener"
}